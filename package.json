{
    "name": "leetcode-api",
    "version": "0.0.1",
    "description": "LeetCode API",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "prepare": "npm run build && npm run docs",
        "dev": "tsup --watch",
        "build": "npm run format && tsup",
        "docs": "typedoc ./src/",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [
        "leetcode",
        "api"
    ],
    "author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "16.11.12",
        "@types/node-fetch": "2",
        "prettier": "2.5.1",
        "tsup": "^5.11.4",
        "typedoc": "0.22.10",
        "typescript": "4.5.3"
    },
    "dependencies": {
        "node-fetch": "2"
    }
}
